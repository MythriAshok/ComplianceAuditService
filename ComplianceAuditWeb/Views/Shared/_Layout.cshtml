<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <link href="~/Content/assets/css/style.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" /> 
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
</head>
<body>
        <div class="sidebar sidebar-collapsed">
            @RenderSection("menupanel", required: false)

        </div>
        <h2>Hello</h2>
        <div class="main_body pull-right none">
            <div class="dashboard">
                <div class="header">
                    <div class="container-fluid">
                        <h1><img src="images/logo_new.png" height="37px" width="232px" alt="" /></h1>
                        <div class="clearfix"></div>
                    </div>
                </div>
            </div>
            <div class="content-sec ML15">
                @RenderBody()
            </div>
            <div class="clearfix"></div>
        </div>
        <div class="clearfix"></div>
        <footer class="footer">
            @RenderSection("footer", required: false)
        </footer>

        <script>
            $(document).ready(function () {
                $('[data-toggle="tooltip"]').tooltip();
            });
        </script>



        //
        <script>
            //$(function(){
            //    $('.queContainer').click(function(){
            //        $('.queContainer.active').addClass('active');
            //        $(this).toggleClass('active');
            //    });
            //});
            //</script>


        <script>

            $('.toggle').on('click', function () {
                $('.sidebar').toggleClass("sidebar-collapsed");
                $('.main_body').toggleClass("new_width");
            });
        </script>



        <script>
            $(function () {
                $('.homeBtn').click(function () {
                    $(this).toggleClass('active');
                });
            });
        </script>

        <style>
            body {
                overflow-y: hidden;
            }
        </style>

        <script src="~/Scripts/jquery-1.10.2.min.js"></script>
        <script src="~/Scripts/bootstrap.min.js"></script>
</body>
</html>

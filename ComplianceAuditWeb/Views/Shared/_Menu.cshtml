@model List<Compliance.DataObject.Menus>
<div class="toggle">
    <span class="icon-bar"></span>
    <span class="icon-bar"></span>
    <span class="icon-bar"></span>
</div>
<div class="left_panel">
    <div class="top_panel">
        <h3>           
            @if(Session["username"]!=null)
            {
                @Session["username"].ToString()<br />
                <span>@Session["emailid"].ToString()</span>
            }
        </h3>
        <p><a href="#"><a href="#">Logout</a></p>
    </div>
    <ul>
        @foreach (var menu in Model)
        {
            <li><a href="@menu.PathUrl">@menu.MenuName <span class="iconnew"><i class=""><img src="~/Content/assets/images/@menu.icon" alt="" /></i></span></a></li>
        }
    </ul>
</div>

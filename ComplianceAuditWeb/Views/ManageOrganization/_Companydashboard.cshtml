@model IEnumerable<ComplianceAuditWeb.Models.ListOfGroupCompanies>
<h2 class="ML15">  Company Dashboard</h2>

<ul class="breadcrumb ML15">
    <li><a href="/Common/dashboard?pid=9">Company Management</a></li>
    <li>Companies</li>
</ul>
<div class="new_section_box">
    <div class="tab-content">
        <div id="home" class="tab-pane fade in active">
            <h2 class="big_hdng" style="text-transform:none; border:0; margin-top:15px;">User Management:</h2>
            @if (TempData["Success"] != null)
            {
                <center>
                    @*<p class="alert alert-success"  style="width:1000px">  @TempData["Company"]</p>*@
                    <p class="alert alert-success" style="width:1000px" id=" successMessage"> @TempData["Company"] @TempData["Success"] </p>
                </center>
            }
            @foreach (var item in Model)
            {
                <div class="col-sm-3 QueList">
                    <div class="queContainer">
                        <a href="@Url.Action("AboutCompany", "ManageOrganization",new { id = item.OrganizationID })">
                            <span class="qIcon"><img src="/UploadedFiles/@item.Logo" alt="" style="height:60px;width:75px"></span>
                            <h3 class="PB25"> @Html.DisplayFor(modelItem => item.CompanyName)</h3>
                        </a>
                        <div class="clearfix"></div>
                    </div>
                </div>
            }

            <div class="clearfix"></div>

        </div>

    </div>

</div>

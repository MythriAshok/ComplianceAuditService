@model ComplianceAuditWeb.Models.OrganizationViewModel

@{
    ViewBag.Title = "ActivateGroupCompany";
}

<ul class="breadcrumb ML15">
    <li><a href="">Group Management</a></li>
    <li>Activate Company</li>
</ul>
<center>
    <h2 class="ML15">Are you sure you want to activate?</h2>
</center>
@using (Html.BeginForm())


{
    @Html.AntiForgeryToken()

<div class="mid_content_sec">
   
    <div class="col-xs-4 subSelect ">
        <center>
            <div class="input-field">



                @Html.HiddenFor(m => m.organization.Organization_Id)

                @Html.LabelFor(m => m.organization.Company_Name, new { @class = "floating-label" })
                @Html.TextBoxFor(m => m.organization.Company_Name, new { @class = "inputMaterial ui - autocomplete - input" })

            </div>
        </center>
    </div>
    
    <ul class="list-inline continueBtn">
        <li>
            @*<input type="submit" value="Activate" class="btn btn-default" />*@
            <button type="submit" class="btn primary-button" id="btnProceed" name="Button" value="Submit">Deactivate<span class="ripple" style="width: 92.6094px; height: 92.6094px; top: 49.5px; left: -19.1875px;"></span></button>
            <button type="submit" class="btn primary-button" id="btnProceed" name="Button" value="Submit">Cancel<span class="ripple" style="width: 92.6094px; height: 92.6094px; top: 49.5px; left: -19.1875px;"></span></button>
        </li>
    </ul>

</div>
        }
  

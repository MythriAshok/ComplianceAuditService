@model IEnumerable<ComplianceAuditWeb.Models.ListOfGroupCompanies>

<div class="new_section_box">
    <div class="tab-content">
        <div id="home" class="tab-pane fade in active">
            <h2 class="big_hdng" style="text-transform:none; border:0; margin-top:15px;">Group Management:</h2>
            @foreach (var item in Model)
            {
                <div class="col-sm-3 QueList">
                    <div class="queContainer">
                        @*@item.OrganizationID = @Session["BranchID"];*@
                        <a href="@Url.Action("AboutCompany", "ManageOrganization",new { id = item.OrganizationID })">
                            <span class="qIcon"><img src="/UploadedFiles/@item.Logo" style="height:50px;width:85px"/></span>
                            <h3 class="PB25"> @Html.DisplayFor(modelItem => item.CompanyName)</h3>
                        </a>
                        <div class="clearfix"></div>
                    </div>
                </div>
            }

            <div class="clearfix"></div>

        </div>

    </div>

</div>

